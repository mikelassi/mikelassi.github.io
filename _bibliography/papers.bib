	keywords = {Stroke, Activities of Daily Living, Adult, Humans, Middle Aged, Electromyography, Arm, Reproducibility of Results, Cohort Studies, Recovery of Function, Treatment Outcome, Stroke Rehabilitation, Motor Activity, Paresis, Pilot Projects},
	pages = {237--253},
	file = {Gains in upper extremity function after stroke via recovery or compensation\: Potential differential effects on amount of real-world limb use:C\:\\Users\\micha\\Zotero\\storage\\6W9VFATP\\tsr1604-237.pdf.pdf:application/pdf;Gains in upper extremity function after stroke via recovery or compensation\: Potential differential effects on amount of real-world limb use:C\:\\Users\\micha\\Zotero\\storage\\KYGBZENC\\tsr1604-237.pdf.pdf:application/pdf},
