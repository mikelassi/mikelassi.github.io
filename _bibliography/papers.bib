	pages = {1058--1067},
	file = {Quantitative EEG for Predicting Upper Limb Motor Recovery in Chronic Stroke Robot-Assisted Rehabilitation:C\:\\Users\\micha\\Zotero\\storage\\65TT6DK8\\TNSRE.2017.2678161.pdf.pdf:application/pdf;Quantitative EEG for Predicting Upper Limb Motor Recovery in Chronic Stroke Robot-Assisted Rehabilitation:C\:\\Users\\micha\\Zotero\\storage\\6E48HJ7I\\TNSRE.2017.2678161.pdf.pdf:application/pdf},
