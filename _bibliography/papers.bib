	author = {Mooney, Ronan A. and Cirillo, John and Stinear, Cathy M. and Byblow, Winston D.},
	month = apr,
	year = {2020},
	pmid = {32066097},
	keywords = {Stroke, Aged, Humans, Middle Aged, Female, Male, Motor learning, Transcranial magnetic stimulation, Transcranial Magnetic Stimulation, Intracortical inhibition, Cerebral Cortex, Learning, Aged, 80 and over, Stroke Rehabilitation, Motor Skills, Evoked Potentials, Motor, Neural Inhibition},
	pages = {791--798},
	file = {Neurophysiology of motor skill learning in chronic stroke:C\:\\Users\\micha\\Zotero\\storage\\APM7WYXN\\j.clinph.2019.12.410.pdf.pdf:application/pdf;Neurophysiology of motor skill learning in chronic stroke:C\:\\Users\\micha\\Zotero\\storage\\SDN2TMSH\\j.clinph.2019.12.410.pdf.pdf:application/pdf},
