	pages = {96--112},
	file = {Springer Full Text PDF:C\:\\Users\\micha\\Zotero\\storage\\7XRW2AB3\\Zheng et al. - 2016 - Exploiting multi-channels deep convolutional neura.pdf:application/pdf},
}

@article{adams_people_2021,
