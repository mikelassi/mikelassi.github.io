	year = {2018},
	keywords = {EMG, Muscle synergies, arm-reaching movements, data pre-processing, factor analysis, upper limb, notion},
	pages = {882--893},
	annote = {The following values have no corresponding Zotero field:publisher: Institute of Electrical and Electronics Engineers Inc.},
	file = {How are Muscle Synergies Affected by Electromyography Pre-Processing?:C\:\\Users\\micha\\Zotero\\storage\\8MUTGAPV\\tnsre.2018.2810859.pdf.pdf:application/pdf},
}

