	annote = {The following values have no corresponding Zotero field:publisher: Society for Neuroscience},
	file = {Muscle synergies obtained from comprehensive mapping of the cortical forelimb representation using stimulus triggered averaging of EMG activity:C\:\\Users\\micha\\Zotero\\storage\\AU84BPGF\\jneurosci.2519-17.2018.pdf.pdf:application/pdf},
