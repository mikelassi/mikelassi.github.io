	month = jan,
	year = {2018},
	pmid = {28990264},
	pmcid = {PMC5725722},
	keywords = {Adult, Humans, Middle Aged, Magnetic Resonance Imaging, Brain Mapping, Brain, Magnetoencephalography, MRI, functional connectivity, beamforming, Brain Waves, co-registration, consistency, magnetoencephalography, MNI template, network analyses, Neural Pathways, Rest},
	pages = {104--119},
