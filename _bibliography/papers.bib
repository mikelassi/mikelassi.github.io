	keywords = {Stroke, stroke, motor cortex, Adult, Aged, Humans, Middle Aged, Female, Male, magnetoencephalography, Aged, 80 and over, Movement, Motor Cortex, beta oscillations, Beta Rhythm, Case-Control Studies, Hand Strength},
	pages = {2053--2058},
	file = {Do movement-related beta oscillations change after stroke?:C\:\\Users\\micha\\Zotero\\storage\\CGPQAL53\\jn.00345.2014.pdf.pdf:application/pdf;Do movement-related beta oscillations change after stroke?:C\:\\Users\\micha\\Zotero\\storage\\NQKTT3LW\\jn.00345.2014.pdf.pdf:application/pdf;Submitted Version:C\:\\Users\\micha\\Zotero\\storage\\D4GUKE5M\\Rossiter et al. - 2014 - Do movement-related beta oscillations change after.pdf:application/pdf},
