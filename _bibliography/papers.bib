	author = {Rondina, Jane M. and Filippone, Maurizio and Girolami, Mark and Ward, Nick S.},
	year = {2016},
	keywords = {Stroke, Features extraction, Gaussian processes, Lesion load, Lesion patterns, Machine learning, Motor impairment, Multiple kernel learning, Patterns of lesion probability, notion},
	pages = {372--380},
	annote = {The following values have no corresponding Zotero field:publisher: Elsevier Inc.accession-num: 27595065},
	file = {Decoding post-stroke motor function from structural brain imaging:C\:\\Users\\micha\\Zotero\\storage\\7UR363LK\\j.nicl.2016.07.014.pdf.pdf:application/pdf},
