	month = jan,
	year = {2018},
	keywords = {Electroencephalography, Mild cognitive impairment, Neurophysiology, Biomarker, Event-related potential},
	pages = {452--460},
	file = {Event-related potentials during sustained attention and memory tasks\: Utility as biomarkers for mild cognitive impairment:C\:\\Users\\micha\\Zotero\\storage\\XHI7SG2Q\\j.dadm.2018.05.007.pdf.pdf:application/pdf;Full Text:C\:\\Users\\micha\\Zotero\\storage\\KEXAS97J\\Waninger et al. - 2018 - Event-related potentials during sustained attentio.pdf:application/pdf},
