	pages = {2217--2226},
	annote = {The following values have no corresponding Zotero field:publisher: Elsevier Ireland Ltdaccession-num: 28987993},
	file = {Corticomuscular coherence in the acute and subacute phase after stroke:C\:\\Users\\micha\\Zotero\\storage\\EFX5DQAP\\j.clinph.2017.08.033.pdf.pdf:application/pdf},
