@article{yarossi_multi-muscle_2017,
	title = {Multi-muscle {TMS} mapping using subject-specific {FEA} models of induced currents},
	volume = {10},
	doi = {10.1016/j.brs.2017.04.032},
	abstract = {Topographical organization of the hand/forearm in the motor cortex (M1)
is known to be non-uniform with most M1 neurons having broad tuning
functions for multiple fingers. Evidence for multi-muscle representations
in M1, often labeled as motor primitives or synergies, is strongly supported
by neurophysiological stimulation-recordings in non-human animal species.
The most practical way to carry out such studies non-invasively in
humans is to use transcranial stimulation to map representations in the
motor system. Of the available techniques, the most attractive at this time
is Transcranial Magnetic Stimulation (TMS) as it offers the best tradeoff
among resolution, depth of penetration, and accurate targeting. However,
traditional TMS map construction does not take into account how stimulation
is dispersed throughout individual cortical geometry and physiology.
In this study, we describe methodology to generate maps of cortical
activation leveraging recent advances in cortical field (E-Field) modeling.
We conducted a TMS (Magstim D702
) mapping experiment targeting 16
muscles of the hand/forearm using a novel non-gridded mapping
approach. Anatomical MRI was used to derive a subject specific headmodeled
mesh with electrical conductivities assigned to the mesh elements
based on known tissue properties. The software BrainStimulator
(SCIRun5) was utilized to compute induced electrical currents for each
stimulation location given the mesh and coil parameters. We describe
algorithms for relating the size of the motor evoked potential to the
induced field strength. Finally, we compare overlap in muscle representations
across the 16 muscles and the likelihood of synergistic responses.},
	number = {4},
