	note = {Publisher: Public Library of Science},
	keywords = {Electroencephalography, Scalp, notion, Cognitive science, Gaussian noise, Sensory perception, Simulation and modeling, Sine waves, Statistical noise},
	pages = {e0181105},
	file = {Full Text PDF:C\:\\Users\\micha\\Zotero\\storage\\3I3UR87J\\Barzegaran and Knyazeva - 2017 - Functional connectivity analysis in EEG source spa.pdf:application/pdf;Functional connectivity analysis in EEG source space\: The choice of method:C\:\\Users\\micha\\Zotero\\storage\\MXSPW243\\journal.pone.0181105.pdf.pdf:application/pdf;Snapshot:C\:\\Users\\micha\\Zotero\\storage\\C3NKATV4\\article.html:text/html},
