	file = {Low-frequency repetitive transcranial magnetic stimulation of the motor cortex in writer's cramp:C\:\\Users\\micha\\Zotero\\storage\\3HYVYKBY\\WNL.52.3.529.pdf.pdf:application/pdf},
}

@article{lomarev_placebo-controlled_2006,
