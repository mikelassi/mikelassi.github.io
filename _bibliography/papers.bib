	year = {2018},
	keywords = {sensory feedback, amputee, biomimetic, encoding strategies, hand prosthesis, implantable electrodes, mechanoreceptors models, neural interface, peripheral nerve stimulation, sensation naturalness, tactile sensory feedback, upper limb amputation, notion},
	pages = {37--45.e7},
