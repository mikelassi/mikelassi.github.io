	pmcid = {PMC6791523},
	keywords = {Stroke, Humans, Brain, cognition, Biomarker, neuroimaging, Recovery of Function, Stroke Rehabilitation, clinical trial, function, human, language, motor, somatosensory, stratification},
	pages = {480--493},
