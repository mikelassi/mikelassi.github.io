	year = {1994},
	keywords = {statistics, analysis of variance, functional anatomy, functional imaging, Gaussian fields, general linear model, statistical parametric maps, notion},
	pages = {189--210},
	annote = {The following values have no corresponding Zotero field:publisher: John Wiley \& Sons, Ltd},
