	keywords = {Transcranial magnetic stimulation, Clinical neurophysiology, Corticomotor conduction, Motor-evoked potentials, notion},
	pages = {858--882},
	annote = {The following values have no corresponding Zotero field:accession-num: 22349304},
	file = {A practical guide to diagnostic transcranial magnetic stimulation\: Report of an IFCN committee:C\:\\Users\\micha\\Zotero\\storage\\QFYBLWP7\\j.clinph.2012.01.010.pdf.pdf:application/pdf},
