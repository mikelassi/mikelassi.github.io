	year = {2014},
	keywords = {EEG, EMG, Motor recovery, Oscillations, CMC, Reorganization, Synchronization, notion},
	pages = {1182--1191},
	annote = {The following values have no corresponding Zotero field:publisher: Elsevier Ireland Ltdaccession-num: 24315544},
