}

@article{shumbayawonda_complexity_2020,
	title = {Complexity changes in preclinical {Alzheimer}'s disease: {An} {MEG} study of subjective cognitive decline and mild cognitive impairment},
	volume = {131},
	doi = {10.1016/j.clinph.2019.11.023},
	abstract = {Objective: To analyse magnetoencephalogram (MEG) signals with Lempel-Ziv Complexity (LZC) to identify the regions of the brain showing changes related to cognitive decline and Alzheimeŕs Disease (AD). Methods: LZC was used to study MEG signals in the source space from 99 participants (36 male, 63 female, average age: 71.82 ± 4.06) in three groups (33 subjects per group): healthy (control) older adults, older adults with subjective cognitive decline (SCD), and adults with mild cognitive impairment (MCI). Analyses were performed in broadband (2–45 Hz) and in classic narrow bands (theta (4–8 Hz), alpha (8–12 Hz), low beta (12–20 Hz), high beta (20–30 Hz), and, gamma (30–45 Hz)). Results: LZC was significantly lower in subjects with MCI than in those with SCD. Moreover, subjects with MCI had significantly lower MEG complexity than controls and SCD subjects in the beta frequency band. Lower complexity was correlated with smaller hippocampal volumes. Conclusions: Brain complexity – measured with LZC – decreases in MCI patients when compared to SCD and healthy controls. This decrease is associated with a decrease in hippocampal volume, a key feature in AD progression. Significance: This is the first study to date characterising the changes of brain activity complexity showing the specific spatial pattern of the alterations as well as the morphological correlations throughout preclinical stages of AD.},
	number = {2},
	journal = {Clinical Neurophysiology},
	author = {Shumbayawonda, Elizabeth and López-Sanz, David and Bruña, Ricardo and Serrano, Noelia and Fernández, Alberto and Maestú, Fernando and Abasolo, Daniel},
	year = {2020},
