	journal = {Neurology},
	author = {Harten, Argonde C. van and Mielke, Michelle M. and Swenson-Dravis, Dana M. and Hagen, Clinton E. and Edwards, Kelly K. and Roberts, Rosebud O. and Geda, Yonas E. and Knopman, David S. and Petersen, Ronald C.},
	month = jul,
