	month = may,
	year = {2021},
	note = {Number: 5
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {stroke, EEG, fractal analysis, neurophysiology, neuroplasticity},
	pages = {592},
	file = {EEG Fractal Analysis Reflects Brain Impairment after Stroke:C\:\\Users\\micha\\Zotero\\storage\\3YM5SJT4\\rubega2021.pdf.pdf:application/pdf;Full Text PDF:C\:\\Users\\micha\\Zotero\\storage\\WP85VAWS\\Rubega et al. - 2021 - EEG Fractal Analysis Reflects Brain Impairment aft.pdf:application/pdf;Snapshot:C\:\\Users\\micha\\Zotero\\storage\\TW94N2WG\\592.html:text/html},
