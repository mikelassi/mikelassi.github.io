	pmcid = {PMC7572485},
	keywords = {Aged, Humans, Electroencephalography, Female, Male, Brain Mapping, Brain, Alzheimer Disease, Sensitivity and Specificity, Early Diagnosis, Aged, 80 and over},
	pages = {17627},
	file = {EEG microstate complexity for aiding early diagnosis of Alzheimer's disease:C\:\\Users\\micha\\Zotero\\storage\\SC5A2DAW\\s41598-020-74790-7.pdf.pdf:application/pdf;EEG microstate complexity for aiding early diagnosis of Alzheimer's disease:C\:\\Users\\micha\\Zotero\\storage\\QIULXEYQ\\s41598-020-74790-7.pdf.pdf:application/pdf;Testo integrale:C\:\\Users\\micha\\Zotero\\storage\\P7N85TMF\\Tait et al. - 2020 - EEG microstate complexity for aiding early diagnos.pdf:application/pdf},
}

@article{smailovic_eeg_2019-1,
	title = {{EEG} time signature in {Alzheimer}´s disease: {Functional} brain networks falling apart},
	volume = {24},
	issn = {2213-1582},
	shorttitle = {{EEG} time signature in {Alzheimer}´s disease},
	url = {https://www.sciencedirect.com/science/article/pii/S2213158219303936},
	doi = {10.1016/j.nicl.2019.102046},
	abstract = {Spontaneous mental activity is characterized by dynamic alterations of discrete and stabile brain states called functional microstates that are thought to represent distinct steps of human information processing. Electroencephalography (EEG) directly reflects functioning of brain synapses with a uniquely high temporal resolution, necessary for investigation of brain network dynamics. Since synaptic dysfunction is an early event and best correlate of cognitive status and decline in patients along Alzheimer's disease (AD) continuum, EEG microstates might serve as valuable early markers of AD. The present study investigated differences in EEG microstate topographies and parameters (duration, occurrence and contribution) between a large cohort of healthy elderly (n = 308) and memory clinic patients: subjective cognitive decline (SCD, n = 210); mild cognitive impairment (MCI, n = 230) and AD (n = 197) and how they correlate to conventional cerebrospinal fluid (CSF) markers of AD. Four most representative microstate maps assigned as classes A, B (asymmetrical), C and D (symmetrical) were computed from the resting state EEGs since it has been shown previously that this is sufficient to explain most of the resting state EEG data. Statistically different topography of microstate maps were found between the controls and the patient groups for microstate classes A, C and D. Changes in the topography of microstate class C were associated with the CSF Aβ42 levels, whereas changes in the topography of class B were linked with the CSF p-tau levels. Gradient-like increase in the contribution of asymmetrical (A and B) and gradient-like decrease in the contribution of symmetrical (C and D) maps were observed with the more severe stage of cognitive impairment. Our study demonstrated extensive relationship of resting state EEG microstates topographies and parameters with the stage of cognitive impairment and AD biomarkers. Resting state EEG microstates might therefore serve as functional markers of early disruption of neurocognitive networks in patients along AD continuum.},
	language = {en},
	urldate = {2022-06-30},
	journal = {NeuroImage: Clinical},
	author = {Smailovic, Una and Koenig, Thomas and Laukka, Erika J and Kalpouzos, Grégoria and Andersson, Thomas and Winblad, Bengt and Jelic, Vesna},
	month = jan,
	year = {2019},
	keywords = {Electroencephalography, Alzheimer's disease, Biomarkers, Cerebrospinal fluid, Functional microstates},
	pages = {102046},
	file = {EEG time signature in Alzheimer´s disease\: Functional brain networks falling apart:C\:\\Users\\micha\\Zotero\\storage\\SBCSC84Y\\j.nicl.2019.102046.pdf.pdf:application/pdf;EEG time signature in Alzheimer´s disease\: Functional brain networks falling apart:C\:\\Users\\micha\\Zotero\\storage\\N7HXIPDB\\j.nicl.2019.102046.pdf.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\micha\\Zotero\\storage\\8PS3QM8T\\S2213158219303936.html:text/html;Testo integrale:C\:\\Users\\micha\\Zotero\\storage\\9RG8TJ5W\\Smailovic et al. - 2019 - EEG time signature in Alzheimer´s disease Functio.pdf:application/pdf},
}

@article{mazzeo_loss_2022,
	title = {Loss of speech and functional impairment in {Alzheimer}'s disease-related primary progressive aphasia: predictive factors of decline},
	volume = {117},
	copyright = {All rights reserved},
	issn = {0197-4580},
	shorttitle = {Loss of speech and functional impairment in {Alzheimer}'s disease-related primary progressive aphasia},
	url = {https://www.sciencedirect.com/science/article/pii/S0197458022001087},
	doi = {10.1016/j.neurobiolaging.2022.05.002},
	abstract = {We aimed to identify features associated with different disease trajectories in Alzheimer's disease (AD)-related primary progressive aphasia (PPA). We considered 23 patients diagnosed with AD-related PPA. All patients underwent neuropsychological evaluation, 18F-Fluorodeoxyglucose-PET brain scan, CSF biomarkers measurement and APOE genotype analysis at baseline and underwent neurological follow-up for a mean time of 3 years. Patients who progressed to total loss of speech (TLoS+) had greater impairment in writing and higher t-tau concentration as compared to TLoS– patients. Patients who progressed to loss of functional autonomy (LoFA+) had greater impairment in single-word comprehension as compared to patients who maintained autonomy in self-care. Furthermore, 18F-FDG-PET SPM analyses revealed different brain metabolic patterns between TLoS+ and TLoS– and between LoFA+ and LoFA–. In conclusion, linguistic profile, CSF t-tau and brain metabolic pattern might be useful tools to predict progression to total loss of speech and loss of functional autonomy in AD-related PPA patients.},
	language = {en},
	urldate = {2022-07-03},
	journal = {Neurobiology of Aging},
	author = {Mazzeo, Salvatore and Polito, Cristina and Lassi, Michael and Bagnoli, Silvia and Mattei, Marta and Padiglioni, Sonia and Berti, Valentina and Lombardi, Gemma and Giacomucci, Giulia and De Cristofaro, Maria Teresa and Passeri, Alessandro and Ferrari, Camilla and Nacmias, Benedetta and Mazzoni, Alberto and Sorbi, Sandro and Bessi, Valentina},
	month = sep,
	year = {2022},
	keywords = {Alzheimer's disease, Amyloid biomarkers, Brain metabolism, Disease progression, Neuropsychology, Primary progressive aphasia},
	pages = {59--70},
}
