	pmcid = {PMC5305670},
	pages = {94--101},
	file = {Accepted Version:C\:\\Users\\micha\\Zotero\\storage\\G4UXXAPF\\Alawieh et al. - 2018 - Factors affecting post-stroke motor recovery Impl.pdf:application/pdf;Factors affecting post-stroke motor recovery\: Implications on neurotherapy after brain injury:C\:\\Users\\micha\\Zotero\\storage\\3RBWYPP4\\j.bbr.2016.08.029.pdf.pdf:application/pdf;Factors affecting post-stroke motor recovery\: Implications on neurotherapy after brain injury:C\:\\Users\\micha\\Zotero\\storage\\WWV9YGTZ\\j.bbr.2016.08.029.pdf.pdf:application/pdf},
