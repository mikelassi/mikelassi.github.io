@article{lamola_neurophysiological_2016-1,
	title = {Neurophysiological {Characterization} of {Subacute} {Stroke} {Patients}: {A} {Longitudinal} {Study}},
	volume = {10},
	issn = {1662-5161},
	shorttitle = {Neurophysiological {Characterization} of {Subacute} {Stroke} {Patients}},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5110528/},
	doi = {10.3389/fnhum.2016.00574},
	abstract = {Various degrees of neural reorganization may occur in affected and unaffected hemispheres in the early phase after stroke and several months later. Recent literature suggests to apply a stratification based on lesion location and to consider patients with cortico-subcortical and subcortical strokes separately: different lesion location may also influence therapeutic response. In this study we used a longitudinal approach to perform TMS assessment (Motor Evoked Potentials, MEP, and Silent Period, SP) and clinical evaluations (Barthel Index, Fugl-Meyer Assessment for upper limb motor function and Wolf Motor Function Test) in 10 cortical-subcortical and 10 subcortical ischemic stroke patients. Evaluations were performed in a window between 10 and 45 days (t0) and at 3 months after the acute event (t1). Our main finding is that 3 months after the acute event patients affected by subcortical stroke presented a reduction in contralateral SP duration in the unaffected hemisphere; this trend is related to clinical improvement of upper limb motor function. In conclusion, SP proved to be a valid parameter to characterize cortical reorganization patterns in stroke survivors and provided useful information about motor recovery within 3 months in subcortical patients.},
	urldate = {2022-04-26},
	journal = {Frontiers in Human Neuroscience},
	author = {Lamola, Giuseppe and Fanciullacci, Chiara and Sgherri, Giada and Bertolucci, Federica and Panarese, Alessandro and Micera, Silvestro and Rossi, Bruno and Chisari, Carmelo},
	month = nov,
	year = {2016},
	pmid = {27899888},
	pmcid = {PMC5110528},
	pages = {574},
	file = {Full Text:C\:\\Users\\micha\\Zotero\\storage\\JDEKD7DY\\Lamola et al. - 2016 - Neurophysiological Characterization of Subacute St.pdf:application/pdf;Neurophysiological Characterization of Subacute Stroke Patients\: A Longitudinal Study:C\:\\Users\\micha\\Zotero\\storage\\TV26WKBE\\fnhum.2016.00574.pdf.pdf:application/pdf;Neurophysiological Characterization of Subacute Stroke Patients\: A Longitudinal Study:C\:\\Users\\micha\\Zotero\\storage\\BST933X6\\fnhum.2016.00574.pdf.pdf:application/pdf},
