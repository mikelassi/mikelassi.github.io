	file = {Quantitative EEG indices of sub-acute ischaemic stroke correlate with clinical outcomes:C\:\\Users\\micha\\Zotero\\storage\\ZXEWDRMA\\j.clinph.2007.07.021.pdf.pdf:application/pdf},
